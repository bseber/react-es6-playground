{
  "name": "react-es6-playground",
  "version": "1.0.0",
  "description": "my react es6 playground",
  "author": "Benjamin Seber <bennyseber@gail.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "6to5ify": "^2.0.0",
    "browserify": "^7.0.3",
    "browserify-shim": "^3.8.1",
    "del": "^1.1.0",
    "gulp": "^3.8.10",
    "gulp-uglify": "^1.0.2",
    "react": "^0.12.2",
    "reactify": "^0.17.1",
    "require-dir": "^0.1.0",
    "vinyl-transform": "^1.0.0"
  },
  "browserify": {
    "transform": [
      "reactify",
      "browserify-shim",
      "6to5ify"
    ]
  },
  "browserify-shim": {
    "react": "global:React"
  },
  "reactify": {
    "es6": true
  },
  "6to5ify": {
    "blacklist": [
      "react"
    ]
  }
}
